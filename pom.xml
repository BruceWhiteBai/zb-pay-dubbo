<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>pers.zb.pay</groupId>
    <artifactId>zb-pay-dubbo</artifactId>
    <version>1.0-SNAPSHOT</version>
    <packaging>pom</packaging>

    <name>zb-pay-dubbo</name>
    <url>http://www.2b2b92b.com</url>
    <description>分布式事务项目</description>

    <modules>
        <!--common-->
        <module>zb-pay-common-config</module>
        <module>zb-pay-common-core</module>

        <!--api-->
        <module>zb-pay-service-user-api</module>
        <module>zb-pay-service-account-api</module>
        <module>zb-pay-service-accounting-api</module>
        <module>zb-pay-service-point-api</module>
        <module>zb-pay-service-message-api</module>
        <module>zb-pay-service-trade-api</module>
        <module>zb-pay-service-notify-api</module>

        <!--service-->
        <module>zb-pay-service-user</module>
        <module>zb-pay-service-account</module>
        <module>zb-pay-service-accounting</module>
        <module>zb-pay-service-point</module>
        <module>zb-pay-service-message</module>
        <module>zb-pay-service-trade</module>
        <module>zb-pay-service-notify</module>

        <!--app-->
        <module>zb-pay-app-message</module>
        <module>zb-pay-app-notify</module>
        <module>zb-pay-app-queue</module>

        <!--web-->
        <module>zb-pay-web-gateway</module>
        <module>zb-pay-web-message</module>
        <module>zb-pay-web-shop</module>
